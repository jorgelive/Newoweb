{% if admin.isGranted('show', object) and admin.hasRoute('resumen') and object.telefono is not empty %}
    <a href="https://api.whatsapp.com/send/?phone={{object.telefono|replace({'-': '', '+': '', '(': '',  ')': '',  ' ': ''}) }}"
       target="_blank"  class="btn btn-sm btn-success"
       title="Whatsapp">
        <i class="fa fa-whatsapp" aria-hidden="true"></i>
        Whatsapp
    </a>
{% endif %}
